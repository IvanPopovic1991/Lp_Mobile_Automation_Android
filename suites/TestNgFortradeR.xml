<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="FortradeR landing pages">
    <listeners>
        <listener class-name="Listeners.ScreenshotListener"/>
        <listener class-name="Listeners.Transform"/>
    </listeners>
    <test name="FortradeR - test cases">
        <classes>
            <class name="TestsFortradeR.pro_Dark_2024_Lp">
                <methods>
                    <include name="didNotGetToken"/>
