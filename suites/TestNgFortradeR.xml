                    <include name="emptyDataAccountRegistration"/>
                    <include name="checkLogoClickability"/>
                    <include name="checkCountryCodeErrorMsg"/>
                    <include name="checkLoginRedirection"/>
                    <include name="errorMessageAgeParameter"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>